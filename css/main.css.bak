@import url('../fonts/myriadpro-reg.css');
@import url('../fonts/myriadpro-bold.css');

button{
	background: transparent;
	border: 0;
	position: relative;
}

button:hover {
	background-position: 0 center;
}

button:active {
	background-position: 0 bottom;
}

input, button, textarea, select, option, label{
	font-family: Arial, sans-serif;
	font-size: 14px;
}

.border-box{
	-ms-box-sizing: border-box;
	-moz-box-sizing: border-box;
	-webkit-box-sizing: border-box;
	box-sizing: border-box;
}

.c{
	clear: both;
	float: none !important;
}

.cl{
	clear: left !important;
}

.fleft{
	float: left !important;
	z-index: 20;
}

.fright{
	float: right !important;
	z-index: 20;
}

.block{
	display: block !important;
}

.table{
	display: table;
	width: 100%;
}

.fixed{
	table-layout: fixed;
}

.clear{
	clear:both !important;
	font-size:0 !important; 
	overflow:hidden;
	height:0;
}

.bold{
	font-weight: bold !important;
}

.ah{
	display: inline-block;
	height: 100%;
	width: 0px !important;
	vertical-align: middle;
	zoom: 1;
}

.oh{
	overflow: hidden;
}

.tac{
	text-align: center;
}

.hidden{
	display: none;
}

.bold{
	font-weight: bold;
}

.selector{
	position: relative;
	display: inline-block;
}

.selector select{
	position: absolute;
	left: 0;
	top: 0;
	height: 100%;
	width: 100%;
	opacity: 0;
	z-index: 10;
}

a{
	color: #A1A1A1;
}

a[href]:hover, a.hover{
	color: #FFF;
}

a.white{
	color: #FFF;
}

a.white[href]:hover, a.white.hover{
	color: #EBF600;
}

h1, h2, h3{
	font-family: 'MyriadProBold';
}

	h1{
		margin-bottom: 20px;
		font-size: 28px;
		line-height: 30px;
	}

	h2{
		font-size: 22px;
		line-height: 25px;
	}

	h3{
		font-size: 16px;
		line-height: 20px;
	}

body{
	background: rgb(54,71,64);
	/*background: url(data:image/svg+xml;base64,);*/
	/*background: -moz-linear-gradient(-45deg,  rgba(54,71,64,1) 0%, rgba(66,72,57,1) 100%);*/
	background: -webkit-gradient(linear, left top, right bottom, color-stop(0%,rgba(54,71,64,1)), color-stop(100%,rgba(66,72,57,1)));
	background: -webkit-linear-gradient(-45deg,  rgba(54,71,64,1) 0%,rgba(66,72,57,1) 100%);
	background: -o-linear-gradient(-45deg,  rgba(54,71,64,1) 0%,rgba(66,72,57,1) 100%);
	background: -ms-linear-gradient(-45deg,  rgba(54,71,64,1) 0%,rgba(66,72,57,1) 100%);
	/*background: linear-gradient(135deg,  rgba(54,71,64,1) 0%,rgba(66,72,57,1) 100%);*/
	font-family: Arial, sans-serif;
	font-size: 13px;
	line-height: 16px;
	color: #FFF;
	min-width: 1000px;
}

#gradient-overlay{
	position: absolute;
	top: 0;
	left: 0;
	min-height: 100%;
	width: 100%;
	background: url(data:image/svg+xml;base64,);
	background: -moz-radial-gradient(center, ellipse cover,  rgba(0,0,0,0) 0%, rgba(0,0,0,0.35) 100%);
	background: -webkit-gradient(radial, center center, 0px, center center, 100%, color-stop(0%,rgba(0,0,0,0)), color-stop(100%,rgba(0,0,0,0.35)));
	background: -webkit-radial-gradient(center, ellipse cover,  rgba(0,0,0,0) 0%,rgba(0,0,0,0.35) 100%);
	background: -o-radial-gradient(center, ellipse cover,  rgba(0,0,0,0) 0%,rgba(0,0,0,0.35) 100%);
	background: -ms-radial-gradient(center, ellipse cover,  rgba(0,0,0,0) 0%,rgba(0,0,0,0.35) 100%);
	background: radial-gradient(ellipse at center,  rgba(0,0,0,0) 0%,rgba(0,0,0,0.35) 100%);
}

#wrapper{
	max-width: 1600px;
	min-width: 1000px;
	overflow: hidden;
	margin: 0 auto;
	padding-bottom: 153px;
}

#header{
	padding-top: 65px;
}

	#header .menu{
		display: none;
		margin-top: 30px;
	}

	#header .contacts{
		margin-right: 20px;
	}

#main{
	margin-top: 50px;
}

#footer{
	position: absolute;
	bottom: 0;
	padding: 40px 0;
	width: 100%;
}

	#footer .search{
		margin: 0 0 10px -36px;
	}

	#footer .column-center .link{

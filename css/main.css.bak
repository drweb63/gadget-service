.login-link{
	background: url('../images/bg/login.png') no-repeat;
	width: 140px;
	height: 42px;
	padding: 4px 0 0 17px;
	cursor: pointer;
}

	.login-link a{
		font-size: 14px;
		text-decoration: none;
		border-bottom: 1px dotted;
		color: #FFF;
	}

	.login-link:hover a{
		color: #EBF600;
	}

	.login-link:after{
		position: absolute;
		right: 20px;
		top: 12px;
		width: 0;
		height: 0;
		border: 5px solid transparent;
		border-top-color: #EBF600;
		content: '';
		z-index: 10;
	}

.reg-link{
	margin: 30px 0 0 17px;
}

.logo{
	margin-top: -11px;
}

.menu{
	height: 52px;
}

	.menu > ul{
		display: table;
		width: 100%;
		height: 100%;
	}

	.menu > ul > li{
		display: table-cell;
		vertical-align: middle;
		padding: 0 5px;
		/*text-align: center;*/
	}

	.menu > ul > li:first-child{
		padding-left: 0;
	}

	.menu > ul > li:last-child{
		padding-right: 0;
	}

	.menu > ul > li > a{
		color: #FFF;
	}

	.menu > ul > li.active{
		padding-right: 7px;
	}

	.menu > ul > li.active.alone{
		padding: 0;
	}

		.menu > ul > li.active.alone div{
			background: url('../images/bg/menu-alone-center.png') 51% 0 no-repeat;
			display: inline-block;
			margin-top: 8px;
		}

		.menu > ul > li.active.alone div:before,
		.menu > ul > li.active.alone div:after{
			background: url('../images/bg/menu-alone-corners.png');
			position: absolute;
			top: 0;
			width: 3px;
			height: 32px;
			content: '';
			z-index: 10;
		}

		.menu > ul > li.active.alone div:before{
			left: -3px;
		}

		.menu > ul > li.active.alone div:after{
			background-position: right 0;
			right: -3px;
		}

		.menu > ul > li.active.alone a{
			display: inline-block;
			position: relative;
			padding: 7px 9px 13px;
			color: #FFF;
			overflow: hidden;
		}

		.menu > ul > li.active.alone a:before,
		.menu > ul > li.active.alone a:after{
			background: url('../images/bg/menu-alone.png');
			position: absolute;
			top: 0;
			width: 100%;
			height: 32px;
			content: '';
			z-index: 10;
		}

		.menu > ul > li.active.alone a:after{
			left: 50%;
			margin-left: 3px;
		}

		.menu > ul > li.active.alone a:before{
			right: 50%;
			margin-right: 2px;
		}

	.menu > ul > li > a[href]:hover{
		color: #EBF600;
	}

.submenu{
	width: 100%;
}

	.submenu > ul > li:first-child{
		background: none;
		padding: 0 6px 0 0;
	}

	.submenu > ul > li:first-child div{
		background: url('../images/bg/menu.png') repeat-x;
		height: 52px;
	}

	.submenu > ul > li:first-child a{
		display: inline-block;
		vertical-align: middle;
	}

		.submenu > ul > li:first-child a:before,
		.submenu > ul > li:first-child a:after{
			background: url('../images/bg/menu-corners.png') no-repeat;
			position: absolute;
			top: 0;
			width: 4px;
			height: 52px;
			content: '';
			z-index: 15;
		}

		.submenu > ul > li:first-child a:before{
			left: -4px;
			background-position: 0 0;
		}

		.submenu > ul > li:first-child a:after{
			right: -4px;
			background-position: right 0;
		}

	.submenu li:first-child div:after,
	.submenu:after{
		background: url('../images/bg/submenu-corners.png');
		position: absolute;
		top: 0;
		width: 6px;
		height: 52px;
		content: '';
		z-index: 10;
	}

	.submenu li:first-child div:after{
		background-position: 0 0;
		right: -6px;
	}

	.submenu:after{
		background-position: 6px 0;
		right: -2px;
	}

	.submenu > ul{
		display: table;
		width: 100%;
	}

	.submenu > ul > li{
		background: url('../images/bg/submenu.png') repeat-x;
		display: table-cell;
		height: 44px;
		vertical-align: middle;
		text-align: center;
	}

	.submenu > ul > li a{
		display: inline-block;
		vertical-align: middle;
		margin: 0 5px;
		text-align: left;
	}

	.submenu > ul > li.active{

	}

	.submenu > ul > li.active div{
		height: 100%;
		border: 4px solid #EBF600;
		z-index: 15;
	}

	.submenu > ul > li.active div:after{
		position: absolute;
		bottom: -12px;
		left: 50%;
		margin-left: -4px;
		width: 0;
		height: 0;
		border: 4px solid transparent;
		border-top-color: #EBF600;
		content: '';
		z-index: 10;
	}

	.submenu > ul > li.active a{
		color: #FFF;
	}


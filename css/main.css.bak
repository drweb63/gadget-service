.curly,
.curly-red{
	display: inline-block;
	height: 3px;
	text-align: center;
	padding-top: 5px;
}

	.curly{
		background: url('../images/bg/curly.png') center 0 no-repeat;
	}

	.curly-red{
		background: url('../images/bg/curly-red.png') center 0 no-repeat;
		color: #F00;
	}

	.curly:before,
	.curly:after,
	.curly-red:before,
	.curly-red:after{
		position: absolute;
		top: -2px;
		width: 2px;
		height: 3px;
		content: '';
		z-index: 10;
	}

	.curly:before,
	.curly:after{
		background: url('../images/bg/curly-corners.png');
	}

	.curly-red:before,
	.curly-red:after{
		background: url('../images/bg/curly-red-corners.png');
	}

	.curly:before,
	.curly-red:before{
		background-position: 0 0;
		left: -2px;
	}

	.curly:after,
	.curly-red:after{
		background-position: right 0;
		right: -2px;
	}
